{
  "name": "aice-utils",
  "version": "0.4.2",
  "description": "AIce utilities",
  "author": "Mik <mik@opla.ai>",
  "homepage": "https://github.com/Opla/aice.js#readme",
  "license": "ISC",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Opla/aice.js.git"
  },
  "scripts": {
    "clean-lib": "rm -r -f lib && mkdir lib",
    "compile": "yarn clean-lib && babel src -s -D -d lib && cp -f ./package.json ./lib/package.json && cp -f ../../LICENSE ./lib/LICENSE",
    "test": "yarn compile && $(yarn bin)/mocha --require babel-core/register --recursive __tests__",
    "lint": "$(yarn bin)/eslint src",
    "coverage": "$(yarn bin)/nyc --reporter=text --reporter=html --report-dir=cov npm run test",
    "report-coverage": "$(yarn bin)/nyc report --reporter=text-lcov > coverage.lcov && $(yarn bin)/codecov",
    "coverage:serve": "rm -rf cov; yarn coverage; npx serve cov"
  },
  "bugs": {
    "url": "https://github.com/Opla/aice.js/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "codecov": "^3.6.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2"
  },
  "dependencies": {
  }
}
